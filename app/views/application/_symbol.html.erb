<svg
  xmlns="http://www.w3.org/2000/svg"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  width="<%= width %>"
  height="<%= height %>"
  viewBox="0 0 <%= width %> <%= height %>"
  class="<%= class_for name, modifiers: modifiers %>"
>
  <use href="#symbol-<%= name %>" xlink:href="#symbol-<%= name %>" viewBox="0 0 <%= width %> <%= height %>"></use>
</svg>
